import processing.core.PApplet;

 
public class setup extends PApplet {
 
    public static void main(String[] args) {
    	PApplet.main("setup");
    }
    
	//public PImage planeImage;
    plane p;
    background b;
    
    public void settings()
    {
        size(500,500);     
    }
 
    public void setup() {
    	p = new plane(this);
    	b = new background(this);
    	//planeImage = loadImage("planeBlue1.png");
    	
    }
 
    public void draw() 
    {
    	background(0);
    	//image(planeImage,0,0);
    	p.display();
    	b.display();
    	b.move();
    	
    }    
}
